{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "buildNpmdoc": "merge2",
    "description": "#### api documentation for  [merge2 (v1.0.3)](https://github.com/teambition/merge2)  [![npm package](https://img.shields.io/npm/v/npmdoc-merge2.svg?style=flat-square)](https://www.npmjs.org/package/npmdoc-merge2) [![travis-ci.org build-status](https://api.travis-ci.org/npmdoc/node-npmdoc-merge2.svg)](https://travis-ci.org/npmdoc/node-npmdoc-merge2)",
    "devDependencies": {
        "electron-lite": "kaizhu256/node-electron-lite#alpha",
        "merge2": "*",
        "utility2": "kaizhu256/node-utility2#alpha"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmdoc/node-npmdoc-merge2",
    "keywords": [
        "documentation",
        "merge2"
    ],
    "license": "MIT",
    "main": "lib.npmdoc_merge2.js",
    "name": "npmdoc-merge2",
    "nameAlias": "npmdoc_merge2",
    "nameOriginal": "npmdoc-merge2",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmdoc/node-npmdoc-merge2"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
        "heroku-postbuild": "npm install \"kaizhu256/node-utility2#alpha\" && utility2 shDeployHeroku",
        "postinstall": "[ ! -f npm_scripts.sh ] || ./npm_scripts.sh postinstall",
        "start": "PORT=${PORT:-8080} utility2 start test.js",
        "test": "(set -e; export PORT=$(utility2 shServerPortRandom); utility2 test test.js)"
    },
    "version": "0.0.3"
}
